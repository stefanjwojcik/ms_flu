@article{gelman_etal_2012,
	Author = {Gelman, Andrew and Silver, Nate and Edlin, Aaron},
	Journal = {Economic Inquiry},
	Number = {2},
	Pages = {321--326},
	Publisher = {Wiley Online Library},
	Review = {Q: How to determine the probability that your vote is decisive in determining an election outcome? A: Prior papers have done this analytically, by calculating the joint distribution of the popular vote margin in each state, then compute the probability of each state being both decisive and locally tied. However, the authors propose a purely empirical form in which they make an estimate of the form y/n, where n is the number of elections in a historical sample that are tied. Because they rare, they suggest y/2000n, where y is the number of elections decided by less than 1000 votes. They embed this idea into regression analysis. E: They run 10000 simulations of the the differences in the vote in each state, using the polling data as the mean in the simulations. They then calculate the probability that the margin of electoral votes is less than the number of electoral votes for that state. They then estimate the probability of votes being decisive by working with the subset of simulations in which the electoral votes in that state are necessary. P: Improves on analytical models.},
	Title = {What is the probability your vote will make a difference?},
	Volume = {50},
	Year = {2012}}

