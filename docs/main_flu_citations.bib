@article{mughan_1987,
	Author = {Mughan, Anthony},
	Journal = {Electoral Studies},
	Number = {3},
	Pages = {195--207},
	Publisher = {Elsevier},
	Review = {This paper examines the British elections in 1987. It tests three forecasting models. The first, somewhat absurd, is based purely on the prior voteshare of a party. The second is based on economic conditions (specifically unemployment in the prior period and GDP in the prior period). The third is based on polling data. The author evaluates the models on the basis of 1) the errors they produce in making a forecast, in which case the polling model performs the best, and 2) in terms of the advance time in which the model can make a prediction. He finds that the polling model can only make an accurate prediction effectively on the day of the election, so concludes that the polling model is less useful. The economic model, on the other hand, can predict the result a fair period of time in advance. The author even attempts to forecast the distribution of seats, despite the obvious difficulty of the first-past-the-post system. He doesn't find much success in doing this.},
	Title = {General election forecasting in Britain: a comparison of three simple models},
	Volume = {6},
	Year = {1987}}

@article{nadeau_etal_2013,
	Author = {Nadeau, Richard and Lewis-Beck, Michael S and B{\'e}langer, {\'E}ric},
	Journal = {Comparative Political Studies},
	Number = {5},
	Pages = {551--573},
	Publisher = {SAGE Publications},
	Review = {Q: Can we predict elections effectively using economic indicators? A: The authors propose that there is a cross-national economic effect on the voting decisions of europeans across these 10 nations. E: The authors estimate a number of models using cross-national surveys in 10 west european countries. They have a balanced sample of 10 countries and 4 panels. The find strong evidence of retrospective economic voting in these countries, as evidenced by logistic regression models of votes for or against the government. They control for ideology, class, religion, partisanship, and other variables. notes: -the context: the ability to accurately predict outcomes of elections signals the effectiveness of democracy in changing according to public will, and says something about generalizeability across contexts. -they find the strongest correlation between growth of GDP and real perceptions of the economy, so in the absence of real perceptions this may be a crucial variable. -they also find a strong correlation with inflation and economic perceptions. operationalization: -a viable coding strategy for cross-national elections is support for the government versus not - a simple binary measure. It allows the choice set to be constructed similarly across cases.},
	Title = {Economics and elections revisited},
	Volume = {46},
	Year = {2013}}

